<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
<!--    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>-->
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>loginModal</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/loginModal</url-pattern>
    </servlet-mapping> 


    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>signupModal</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signupModal</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>newdonor</servlet-name>
        <servlet-class>user</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>newdonor</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>hospitalReg</servlet-name>
        <servlet-class>hospital</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hospitalReg</servlet-name>
        <url-pattern>/hospital</url-pattern>
    </servlet-mapping>

</web-app>
